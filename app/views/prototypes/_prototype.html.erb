<div class="card">
  <div class="card__body">
  <% if prototype.image.attached? %>
      <%= link_to image_tag(prototype.image, class: :card__img),  prototype_path(prototype), method: :get %> 
  <% end %>
    <%= link_to prototype.title, prototype_path(prototype.id), method: :get, class: :card__title%>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{prototype.user.name}", user_path(prototype.user_id), class: :card__user %>
  </div>
</div>

